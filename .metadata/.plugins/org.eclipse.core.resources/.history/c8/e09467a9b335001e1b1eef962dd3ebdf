package bike.bikeMain;

import bike.Bike;
import bike.Wheel;

public class bike_main {

public static void main(String[] args) {
		
		Bike bike1 = new Bike();
		bike1.setName("세발자전거");
		bike1.setWheel(3);
		System.out.println(bike1);
		
		Bike bike2 = new Bike();
		bike2.setName("MTB");
		bike2.setWheel(2);
		System.out.println(bike2);
		
		Bike bike3 = new Bike();
		bike3.setName("CF");
		bike3.setWheel(2);
		System.out.println(bike3);

		Wheel wheel1 = new Wheel(3);
		System.out.println(wheel1);
		
		Wheel wheel2 = new Wheel(2);
		System.out.println(wheel2);
		
		Wheel wheel3 = new Wheel(2);
		System.out.println(wheel3);
	}

}
