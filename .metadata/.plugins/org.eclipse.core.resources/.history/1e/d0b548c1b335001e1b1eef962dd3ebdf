package bike.bikeMain;

import bike.Bike;
import bike.Wheel;

public class bike_main {

public static void main(String[] args) {
		
		Bike bk1 = new Bike();
		bk1.setName("세발자전거");
		bk1.setWheel(3);
		System.out.println(bk1);
		
		Bike bk2 = new Bike();
		bk2.setName("MTB");
		bk2.setWheel(2);
		System.out.println(bk2);
		
		Bike bike3 = new Bike();
		bike3.setName("CF");
		bike3.setWheel(2);
		System.out.println(bike3);

		Wheel wheel1 = new Wheel(3);
		System.out.println(wheel1);
		
		Wheel wheel2 = new Wheel(2);
		System.out.println(wheel2);
		
		Wheel wheel3 = new Wheel(2);
		System.out.println(wheel3);
	}

}
