package Coffee;

import Coffee.CoffeeEtc.CoffeeBeans;
import Coffee.type.Americano;
import Coffee.type.Latte;
import Coffee.type.Machiatto;

public class Coffee {
	
	Americano am;
	Latte lt;
	Machiatto mc;
	CoffeeBeans beans;
	
	public Americano getAm() {
		return am;
	}
	public void setAm(Americano am) {
		this.am = am;
	}
	public Latte getLt() {
		return lt;
	}
	public void setLt(Latte lt) {
		this.lt = lt;
	}
	public Machiatto getMc() {
		return mc;
	}
	public void setMc(Machiatto mc) {
		this.mc = mc;
	}
	public CoffeeBeans getBeans() {
		return beans;
	}
	public void setBeans(CoffeeBeans beans) {
		this.beans = beans;
	}
	
	@Override
	public String toString() {
		return "Coffee [am=" + am + ", lt=" + lt + ", mc=" + mc + ", beans=" + beans + "]";
	}
		
}