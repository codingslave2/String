package Coffee;

import org.springframework.beans.factory.annotation.Autowired;

public class Coffee {
	
	@Autowired
	
	Coffee coffee;
	int Price;
	CoffeeBeans cbean;
	Etc etc;

}



class CoffeeBeans{
	
	String name;
	int pack;
	
	
}

class Etc{
	
	String name;
	int ml;
	
	
	public void setName(String name) {
		this.name = name;
	}
	public void setMl(int ml) {
		this.ml = ml;
	}
	
	@Override
	public String toString() {
		return "Etc [name=" + name + ", ml=" + ml + "]";
	}
	
	
}