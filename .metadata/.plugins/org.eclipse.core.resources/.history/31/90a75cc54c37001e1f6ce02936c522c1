package Phone;

public class Camera {
	
	String productor;
	String name;
	int pixel;

	public String getProductor() {
		return productor;
	}
	public void setProductor(String productor) {
		this.productor = productor;
	}
	public int getPixel() {
		return pixel;
	}
	public void setPixel(int pixel) {
		this.pixel = pixel;
	}
	
	@Override
	public String toString() {
		return "Camera [productor=" + productor + ", pixel=" + pixel + "]";
	}	
	
}
