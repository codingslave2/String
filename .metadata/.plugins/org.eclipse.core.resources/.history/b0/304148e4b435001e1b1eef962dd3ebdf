package bike.bikeMain;

import bike.Bike;
import bike.Wheel;

public class bike_main {

public static void main(String[] args) {
		
		Bike bk1 = new Bike();
		bk1.setName("세발자전거");
		bk1.setWheel(3);
		System.out.println(bk1);
		
		Bike bk2 = new Bike();
		bk2.setName("MTB");
		bk2.setWheel(2);
		System.out.println(bk2);
		
		Bike bk3 = new Bike();
		bk3.setName("CF");
		bk3.setWheel(2);
		System.out.println(bk3);

		Wheel wh1 = new Wheel(3);
		System.out.println(wh1);
		
		Wheel wh2 = new Wheel(2);
		System.out.println(wh2);
		
		Wheel wh3 = new Wheel(2);
		System.out.println(wh3);
	}

}
