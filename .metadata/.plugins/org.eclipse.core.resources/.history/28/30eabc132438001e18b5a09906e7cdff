package anno_p;

public class RCCar {
	
	String name;
	Motor mt;
	RCModel rc1, rc2, rc3, rc4;
	
}

class RCWheel{
	
	String name;
	int size;
	public void setName(String name) {
		this.name = name;
	}
	public void setSize(int size) {
		this.size = size;
	}
	
	@Override
	public String toString() {
		return "RCWheel [name=" + name + ", size=" + size + "]";
	}
	
}
