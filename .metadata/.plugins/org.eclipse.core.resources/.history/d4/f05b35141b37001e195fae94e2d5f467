package di_p;

import org.springframework.context.support.AbstractApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import cfshop.CoffeeShop;

public class Lookup_main {

	public static void main(String[] args) {
			
		AbstractApplicationContext context = new ClassPathXmlApplicationContext("di_xml/lookup.xml");

		CoffeeShop co1 = context.getBean("coffee", CoffeeShop.class);
		CoffeeShop co2 = context.getBean("coffee", CoffeeShop.class);

		System.out.println("주문: " + co1.order());
		System.out.println("주문: " + co2.order());
		
	}

}
