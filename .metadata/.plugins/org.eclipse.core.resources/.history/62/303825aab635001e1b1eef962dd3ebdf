package bike;

public interface BikeMovement {
	
	public static void main(String[] args) {
		
		Bike bk1 = new Bike();
		Bike bk2 = new Bike();
		Bike bk3 = new Bike();
		

		
		bk1.setName("세발자전거");
		bk2.setName("MTB");
		bk3.setName("CF");
		
		System.out.println(bk1);
		System.out.println(bk2);
		System.out.println(bk3);
		
		System.out.println("---------------------------------------------");
		
	}
	
}
