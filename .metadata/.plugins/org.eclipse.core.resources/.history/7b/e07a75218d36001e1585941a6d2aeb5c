package Student;

import java.util.ArrayList;
import java.util.List;

public class Student {
    
	String name;
	List<String> subject1 = new ArrayList<>();
	List<String> subject2 = new ArrayList<>();
	List<String> subject3 = new ArrayList<>();
	List<Integer> scores = new ArrayList<>();
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public List<String> getSubject1() {
		System.out.println("국어: ");
		return subject1;
	}
	public void setSubject1(List<String> subject1) {
		this.subject1 = subject1;
	}
	public List<String> getSubject2() {
		System.out.println("영어: ");
		return subject2;
	}
	public void setSubject2(List<String> subject2) {
		this.subject2 = subject2;
	}
	public List<String> getSubject3() {
		return subject3;
	}
	public void setSubject3(List<String> subject3) {
		this.subject3 = subject3;
	}
	public List<Integer> getScores() {
		return scores;
	}
	public void setScores(List<Integer> scores) {
		this.scores = scores;
	}
	@Override
	public String toString() {
		return "Student [name=" + name + ", subject1=" + subject1 + ", subject2=" + subject2 + ", subject3=" + subject3
				+ ", scores=" + scores + "]";
	}

	


}
