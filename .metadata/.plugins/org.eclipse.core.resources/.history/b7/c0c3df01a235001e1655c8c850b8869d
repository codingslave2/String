package di_p.main;

import di_p.Bike;

public class bike_main {

	public static void main(String[] args) {
		
		Bike bk1 = new Bike();
		Bike bk2 = new Bike();
		Bike bk3 = new Bike();
		
		bk1.setName("세발자전거");
		bk1.setHandle("핸들");
		bk1.setWheel(3);
		
		bk2.setName("MTB");
		bk2.setHandle("핸들");
		bk2.setWheel(2);
		
		
		
		System.out.println(bk1);
		System.out.println(bk2);

	}

}
