package Restaurant.Order;

import org.springframework.stereotype.Component;

@Component
public class Order {
	
	public String order1(String a, int x) {
		
		System.out.println("1번 오더: " + a + x);
		return a;
		
	}
	
	public String order1(String b, int y) {
		
		System.out.println("1번 오더: " + b + y);
		return b;
		
	}

}
