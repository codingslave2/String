package bike;

public class Backet {

	int Backet = 0;

	public Backet(int Backet) {
		this.Backet = Backet;
	}

	public int getBacket() {
		return Backet;
	}

	public void setBacket(int backet) {
		System.out.println("Backet: " + backet);
        System.out.println("Backet: " + Backet);
        System.out.println("Backet: " + this.Backet);
        System.out.println("Backet: " + this.getBacket());
        System.out.println("Backet: " + this.getClass());
        System.out.println("Backet: " + this.hashCode());
        System.out.println("Backet: " + this.toString());
        System.out.println("Backet: " + this.getClass().getName());
        System.out.println("Backet: " + this.getClass().getSimpleName());
        System.out.println("Backet: " + this.getClass().getSuperclass());
        System.out.println("Backet: " + this.getClass().getInterfaces());
        System.out.println("Backet: " + this.getClass().getDeclaredFields());
        System.out.println("Backet: " + this.getClass().getDeclaredMethods());
		this.Backet = backet;
	}

	public void print() {
		System.out.println("바구니: " + Backet);
	}
	
}
