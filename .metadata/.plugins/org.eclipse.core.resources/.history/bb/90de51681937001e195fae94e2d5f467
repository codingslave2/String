package di_p;

import org.springframework.context.support.AbstractApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import cfshop.CoffeeShop;

public class Lookup_main {

	public static void main(String[] args) {
			
		AbstractApplicationContext context = new ClassPathXmlApplicationContext("di_mxl/lookup.xml");

		CoffeeShop shop = context.getBean("shop", CoffeeShop.class);

		System.out.println("주문: " + shop.order());
		
	}

}
