package component;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import component.sub1.Mouse;
import component.sub2.Monitor;
import jakarta.annotation.Resource;

@Component
public class DESKTop {

	@Autowired
	block bc;
	@Resource
	Mouse mouse;
	
	Monitor monitor;
	String name = "마이피씨";
	@Override
	public String toString() {
		return "DeskTop [name=" + name + ", bc=" + bc + ", mouse=" + mouse + ", monitor=" + monitor + "]";
	}
	
	
	
}
