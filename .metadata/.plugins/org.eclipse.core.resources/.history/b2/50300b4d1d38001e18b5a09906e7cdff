package Coffee;

import org.springframework.beans.factory.annotation.Autowired;

public class Coffee {
	
	@Autowired
	
	Coffee coffee;
	int Price;
	CoffeeBeans cbean;
	Etc etc;
	
	
	public void setCoffee(Coffee coffee) {
		this.coffee = coffee;
	}
	public void setPrice(int price) {
		Price = price;
	}
	public void setCbean(CoffeeBeans cbean) {
		this.cbean = cbean;
	}
	
	@Autowired(required = false)
	public void setEtc(Etc etc) {
		this.etc = etc;
	}
	@Override
	public String toString() {
		return "Coffee [coffee=" + coffee + ", Price=" + Price + ", cbean=" + cbean + ", etc=" + etc + "]";
	}
	

}




class CoffeeBeans{
	
	String name;
	int pack;
	public void setName(String name) {
		this.name = name;
	}
	public void setPack(int pack) {
		this.pack = pack;
	}
	
	
	
}

class Etc{
	
	String name;
	int ml;
	
	
	public void setName(String name) {
		this.name = name;
	}
	public void setMl(int ml) {
		this.ml = ml;
	}
	
	@Override
	public String toString() {
		return "Etc [name=" + name + ", ml=" + ml + "]";
	}
	
	
}