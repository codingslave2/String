package anno_p;

import org.springframework.context.annotation.Bean;

public class AnnoConfig {
	
	@Bean
	BiHandle hd() {
		return new BiHandle("일자핸들", "산악용");
		
	}
	
	@Bean
	BiWheel wh() {
		return new BiWheel("디스크", 20);
		
	}

	@Bean
	BiCycle mtb() {
		return new BiCycle("삼천리", null, null);
		
	}
	
}
