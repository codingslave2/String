package di_p;

public class Bike {
	
	String name;
	int wheel;
	
	
	
	
	public String getName() {
		return name;
	}




	public void setName(String name) {
		this.name = name;
	}




	public int getWheel() {
		return wheel;
	}




	public void setWheel(int wheel) {
		this.wheel = wheel;
	}




	@Override
	public String toString() {
		return "Bike [name=" + name + ", wheel=" + wheel + "]";
	}
	
	
	
	
	
}
