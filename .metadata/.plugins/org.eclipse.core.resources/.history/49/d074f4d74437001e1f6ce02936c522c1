package Phone;

public class Display {
	
	String productor;
	int size;

	public Display(String productor, int size) {
		super();
		this.productor = productor;
        this.size = size;
	}
	
	public String getProductor() {
		return productor;
	}
	public void setProductor(String productor) {
		this.productor = productor;
	}
	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}
	
	@Override
	public String toString() {
		return "Display [productor=" + productor + ", size=" + size + "]";
	}	
	
}
